datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/db"
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id          String @id @default(uuid())
  name        String
  password    String
  email       String?
  admin       Boolean @default(false)

  @@map("users")
}

model Planet {
  id              Int @id @default(autoincrement())
  name            String
  description     String
  image           String?
  dimension       String?
  first_episode   String?
  some_habitants  String?

  @@map("planets")
}
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model User {
  id          String @id @default(uuid())
  name        String
  password    String
  email       String?
  admin       Boolean @default(false)

  @@map("users")
}

model Planet {
  id              Int @id @default(autoincrement())
  id_reference    String
  image           String
  description     String?

  @@map("planets")
}